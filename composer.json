{
    "name": "directorytree/dummy",
    "type": "library",
    "license": "MIT",
    "archive": {
        "exclude": [
            "/tests"
        ]
    },
    "autoload": {
        "psr-4": {
            "DirectoryTree\\Dummy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DirectoryTree\\Dummy\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^2.0|^3.0",
        "laravel/pint": "^1.0",
        "fakerphp/faker": "^1.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
