{
    "name": "directorytree/fakeable",
    "type": "library",
    "license": "MIT",
    "archive": {
        "exclude": [
            "/tests"
        ]
    },
    "autoload": {
        "psr-4": {
            "DirectoryTree\\Fakeable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DirectoryTree\\Fakeable\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^3.0",
        "laravel/pint": "^1.18",
        "fakerphp/faker": "^1.0",
        "illuminate/support": "^11.0",
        "illuminate/database": "^11.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
